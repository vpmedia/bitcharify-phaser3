    "eslint-plugin-jsdoc": "^46.5.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint-fix": "eslint --fix \"**/*.{js,jsx}\"",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
